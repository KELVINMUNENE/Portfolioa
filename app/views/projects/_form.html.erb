<%= render "layouts/errors", :object => @project %>

<%= form_for @project do |f| %>
<div class="form-group">
  <%= f.label :name %>
  <%= f.text_field :name %>
</div>
<div class="form-group">
  <%= f.label :project_link %>
  <%= f.text_field :project_link %>
</div>
<div class="form-group">
  <%= f.label :description %>
  <%= f.text_field :description %>
</div>
<div class="form-group">
  <% Skill.all.each do |skill| %>
    <%= check_box_tag "skills[]", skill.id %><%=skill.name%>
    <%end%>
</div><br>
<div class="form-group">
  <%= f.label :picture %>
  <%= f.file_field :picture %>
</div>
<br>
  <%= f.submit %>
<% end %>
